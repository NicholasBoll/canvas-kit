+            });
+          });
+
+          it('should close the modal', () => {
+            h.modal.get().should('not.be.visible');
+          });
+        });
+
+        context('when the overlay is clicked', () => {
+          beforeEach(() => {
+            h.modal
+              .get()
+              .parent() // overlay
+              .click('top');
+          });
